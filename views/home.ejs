<section class="mt-5">
  <div class="row  cols-md-3 ">
    <% for( let index = 0; index < product.length; index++ ) { %>
    
    <div class="col">
      <div class="card" style=" width: 150px;">
           <img src="<%=product[index].img %>" class="card-img-top" alt="Skyscrapers" style="height: 150px; object-fit: contain;" /> 
        <div class="card-body">
          <h5 class="card-title"><%= product[index].price%> </h5>
         
        </div>
        <div class="card-footer">
          <small class="text-muted"> <%= product[index].description %> </small>
        </div>
      </div>

    </div>
    
    <% } %>
